namespace: "drake::automotive"

element {
     name: "forward_acceleration"
     doc: "acceleration in forward (heading) direction"
     doc_units: "m/s^2"
     default_value: "0.0"
}
element {
     name: "curvature"
     doc: "signed curvature of turn (positive is leftward, rotating counterclockwise)"
     doc_units: "1/m"
     default_value: "0.0"
}
